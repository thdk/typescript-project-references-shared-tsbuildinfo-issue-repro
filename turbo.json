{
    "$schema": "https://turborepo.com/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**"
            ],
            "cache": true
        },
        "typecheck": {
            "dependsOn": [
                "build"
            ],
            "outputs": [
                "dist/**/*.d.ts",
                "dist/**/*.d.cts",
                "dist/**/*.d.mts",
                "dist/**/*.d.ts.map",
                "dist/**/*.d.cts.map",
                "dist/**/*.d.mts.map",
                "dist/**/*.tsbuildinfo",
                "out-tsc/**"
            ],
            "cache": true
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ]
        },
        "format": {},
        "test": {
            "dependsOn": [
                "^build"
            ]
        }
    }
}